def Run(pos,res):
    for [r,pk] in res:
        n = len(r)
        li = [r[1]-r[0]]
        cht(pk > 3)
        mt1(pos,1,r[0],pk)
        for i in range(1,n):
            if i < n-1:
                dire = r[i+1] - r[i]
                skip = li[i-1] == dire
                li.append(dire)
                if skip:
                    continue
            mt1(pos,1 if not i else 0 if i == n-1 else 2,r[i],pk)

