def ps(p,ad,y,x):
    pos = {}
    sy,sx = [[],[]],[[],[]]
    for tool in range(2):
        p1 = p[tool]
        for i in range(2):
            sy[tool].append((p1[1][i] - p1[0][i]) / (y-1))
            sx[tool].append((p1[2][i] - p1[0][i]) / (x-1))
    for i in range(y*x):
        pos[i] = [[],[],ad + i]
        for tool in range(2):
            for j in range(6):
                err = (sy[tool][j] * (i//x) + sx[tool][j] * (i%x)) if j < 2 else 0
                pos[i][tool].append(p[tool][0][j] + err)
    return pos
